[user]
    name = James D
    email = jamesgecko@gmail.com
[core]
    excludesfile = /Users/jdent/.gitignore_global
[color]
    ui = true
[credential]
    helper = osxkeychain
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    root = rev-parse --show-toplevel
    currentbranch = rev-parse --abbrev-ref HEAD
